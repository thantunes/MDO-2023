  },
  "image#brand1": {
    "props": {
      "src": "/arquivos/andreaconsini-brand.png"
    }
  },
  "image#brand2": {
    "props": {
      "src": "/arquivos/armatti-brand-2.png"
    }
  },
  "image#brand3": {
    "props": {
      "src": "/arquivos/caffe-brand.png"
    }
  },
  "image#brand4": {
