  "rich-text#shelf-title-novidades": {
    "props": {
      "text": "Produtos em destaque",
      "blockClass": "shelfTitle"
    }
  },

  "flex-layout.row#shelfnovidades": {
    "children": ["list-context.product-list#novidades"]
  },
  "list-context.product-list#novidades": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#novidades"],
    "props": {
      "orderBy": "OrderByReleaseDateDESC",
      "collection": "137"
    }
  },
  "slider-layout#novidades": {
    "props": {
      "itemsPerPage": {
        "desktop": 5,
        "tablet": 3,
        "phone": 1
      },
      "infinite": true,
      "fullWidth": false,
      "blockClass": "shelf"
    }
  },

  "flex-layout.row#banner-full-centro-home": {
    "children": ["image#banner-full-centro-home"],
    "props": {
      "blockClass": "banner-full-centro-home"
    }
  },
  "image#banner-full-centro-home": {
    "props": {
      "src": "/arquivos/Banner-Chris-Flores-B2B2---meio.png"
    }
  },

  "info-card#home": {
    "props": {
      "id": "info-card-home",
      "isFullModeStyle": false,
      "textPosition": "left",
      "imageUrl": "https://storecomponents.vteximg.com.br/arquivos/banner-infocard2.png",
      "headline": "Clearance Sale",
      "callToActionText": "DISCOVER",
      "callToActionUrl": "/sale/d",
      "blockClass": "info-card-home",
      "textAlignment": "center"
    }
  },
  "rich-text#shelf-title-naopodefaltar": {
    "props": {
      "text": "Promoção",
      "blockClass": "shelfTitle"
    }
  },
  "flex-layout.row#shelfnaopodefaltar": {
    "children": ["list-context.product-list#naopodefaltar"]
  },
  "list-context.product-list#naopodefaltar": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#naopodefaltar"],
    "props": {
      "orderBy": "OrderByReleaseDateDESC",
      "collection": "159"
    }
  },
  "slider-layout#naopodefaltar": {
    "props": {
      "itemsPerPage": {
        "desktop": 5,
