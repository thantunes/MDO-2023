{
  "store.home": {
    "blocks": [
      "list-context.image-list#banner-principal-home",
      "SearchFilterColor",
      "slider-layout#marcas",
      "list-context.image-list#Brands",
      "__fold__.experimentalLazyAssets",
      "rich-text#shelf-title",
      "flex-layout.row#shelf",
      "flex-layout.row#3banners-home",
      "rich-text#shelf-title-novidades",
      "flex-layout.row#shelfnovidades",
      "flex-layout.row#banner-full-centro-home",
      "rich-text#shelf-title-naopodefaltar",
      "flex-layout.row#shelfnaopodefaltar"
    ],
    "parent": {
      "challenge": "challenge.trade-policy-condition"
    }
  },

  "challenge.trade-policy-condition": {
    "props": {
      "redirectPath": "/login",
      "defaultContentVisibility": "hidden"
    }
  },

  "list-context.image-list#banner-principal-home": {
    "children": ["slider-layout#demo-images"],
    "props": {
      "height": 720,
      "images": [
        {
          "image": "https://mercadaodosoculosb2b.vteximg.com.br/arquivos/Banner-Chris-Flores-B2B1---hero.png"
        }
      ]
    }
  },
  "slider-layout#demo-images": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "autoplay": {
        "timeout": 5000,
        "stopOnHover": true
      },

      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "blockClass": "carousel"
    }
  },

  "rich-text#shelf-title": {
    "props": {
      "text": "Lançamentos",
      "blockClass": "shelfTitle"
    }
  },
  "flex-layout.row#shelf": {
    "children": ["list-context.product-list#demo1"]
  },
  "list-context.product-list#demo1": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#demo-products"],
    "props": {
      "orderBy": "OrderByNameASC",
      "collection": "147"
    }
  },
  "slider-layout#demo-products": {
    "props": {
      "itemsPerPage": {
        "desktop": 5,
        "tablet": 3,
        "phone": 1
      },
      "infinite": true,
      "fullWidth": false,
      "blockClass": "shelf"
    }
  },

  "flex-layout.row#3banners-home": {
    "children": [
      "image#3bannreshome1",
      "image#3bannreshome2",
      "image#3bannreshome3"
    ],
    "props": {
      "blockClass": "banners-3-homepage"
    }
  },

  "image#3bannreshome1": {
    "props": {
      "src": "/arquivos/para-ele-v4.png",
      "url": "/oculos-de-grau/genero_Masculino"
    }
  },

  "image#3bannreshome2": {
    "props": {
      "src": "/arquivos/para-ela-v4.png",
      "url": "/oculos-de-grau/genero_Feminino"
    }
  },

  "image#3bannreshome3": {
    "props": {
      "src": "/arquivos/para-kids-v4.png",
      "url": "/oculos-de-grau/genero_Infantil"
    }
  },

